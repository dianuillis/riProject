<censor wordnetid="109903936" confidence="0.8">
<diplomat wordnetid="110013927" confidence="0.8">
<link xlink:type="simple" xlink:href="../211/1785211.xml">
Scipio Africanus</link></diplomat>
</censor>
</causal_agent>
</official>
</worker>
</leader>
</general_officer>
</commissioned_military_officer>
</consul>
