<link xlink:type="simple" xlink:href="../083/8083.xml">
Dr. Dre</link></artist>
</manufacturer>
</rapper>
</person>
, <link xlink:type="simple" xlink:href="../328/148328.xml">
Ice Cube</link>, <link xlink:type="simple" xlink:href="../589/26589.xml">
The Red Hot Chili Peppers</link> and <person wordnetid="100007846" confidence="0.9508927676800064">
<rapper wordnetid="110507482" confidence="0.9173553029164789">
<manufacturer wordnetid="110292316" confidence="0.9173553029164789">
