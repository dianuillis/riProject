<detective wordnetid="110009276" confidence="0.8">
<link xlink:type="simple" xlink:href="../604/15604.xml">
James Bond</link></detective>
</killer>
</murderer>
</causal_agent>
</defender>
</wrongdoer>
</assassin>
</golfer>
