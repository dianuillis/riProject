<link xlink:type="simple" xlink:href="../467/8295467.xml">
Jordan Maxwell</link></causal_agent>
</astrologer>
</forecaster>
</intellectual>
</theorist>
</person>
</physical_entity>
</visionary>
, and <physical_entity wordnetid="100001930" confidence="0.8">
<politician wordnetid="110451263" confidence="0.8">
<person wordnetid="100007846" confidence="0.8">
<leader wordnetid="109623038" confidence="0.8">
