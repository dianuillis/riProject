<recipient wordnetid="109627906" confidence="0.8">
<associate wordnetid="109816771" confidence="0.8">
<link xlink:type="simple" xlink:href="../507/307507.xml">
Stearns</link></associate>
</recipient>
</scientist>
</acquirer>
