<district wordnetid="108552138" confidence="0.8">
<urban_area wordnetid="108675967" confidence="0.8">
<link xlink:type="simple" xlink:href="../043/16043.xml">
Jerusalem</link></urban_area>
</district>
</geographical_area>
</seat>
</see>
</area>
</center>
</city>
</capital>
</municipality>
