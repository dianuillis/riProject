<group wordnetid="100031264" confidence="0.8">
<link xlink:type="simple" xlink:href="../769/26769.xml">
South America</link></group>
</economy>
</system>
, <link xlink:type="simple" xlink:href="../239/9239.xml">
Europe</link>, <system wordnetid="108435388" confidence="0.8">
<economy wordnetid="108366753" confidence="0.8">
<group wordnetid="100031264" confidence="0.8">
<link xlink:type="simple" xlink:href="../689/689.xml">
Asia</link></group>
