<democracy wordnetid="106217103" confidence="0.8">
<link xlink:type="simple" xlink:href="../750/3434750.xml">
American</link></democracy>
</group>
</political_unit>
</state>
</colony>
</political_orientation>
</social_group>
</body>
 <link xlink:type="simple" xlink:href="../935/161935.xml">
animator</link>, <link xlink:type="simple" xlink:href="../753/753.xml">
actor</link>, <link xlink:type="simple" xlink:href="../673/95673.xml">
voice actor</link>, <link xlink:type="simple" xlink:href="../353/45353.xml">
writer</link>, <link xlink:type="simple" xlink:href="../181/506181.xml">
