<novelist wordnetid="110363573" confidence="0.8">
<link xlink:type="simple" xlink:href="../479/2838479.xml">
Amy Jenkins</link></novelist>
</writer>
</causal_agent>
</person>
</communicator>
</physical_entity>
, based on <physical_entity wordnetid="100001930" confidence="0.8">
<expert wordnetid="109617867" confidence="0.8">
<peer wordnetid="109626238" confidence="0.8">
<person wordnetid="100007846" confidence="0.8">
<colleague wordnetid="109935990" confidence="0.8">
