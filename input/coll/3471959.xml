<link xlink:type="simple" xlink:href="../723/880723.xml">
Cylons</link></mechanism>
</device>
</instrumentality>
</artifact>
</automaton>
 of <music wordnetid="107020895" confidence="0.8">
<beginning wordnetid="100235435" confidence="0.8">
<change wordnetid="100191142" confidence="0.8">
<crew wordnetid="108273167" confidence="0.8">
<music_genre wordnetid="107071942" confidence="0.8">
<change_of_state wordnetid="100199130" confidence="0.8">
<classical_music wordnetid="107025900" confidence="0.8">
<auditory_communication wordnetid="107109019" confidence="0.8">
