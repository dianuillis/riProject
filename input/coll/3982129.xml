<link xlink:type="simple" xlink:href="../420/180420.xml">
Forest Lawn Memorial Park Cemetery</link></museum>
</geographical_area>
</depository>
</cemetery>
</tract>
</location>
</facility>
</artifact>
</region>
</site>
 in <region wordnetid="108630985" confidence="0.8">
<vicinity wordnetid="108641113" confidence="0.8">
