<singer wordnetid="110599806" confidence="0.9173553029164789">
<artist wordnetid="109812338" confidence="0.9508927676800064">
<pianist wordnetid="110430665" confidence="0.9173553029164789">
<link xlink:type="simple" xlink:href="../328/179328.xml">
Burt Bacharach</link></pianist>
</artist>
</singer>
</composer>
 and <physical_entity wordnetid="100001930" confidence="0.8">
