<link xlink:type="simple" xlink:href="../980/6182980.xml">
Itamos</link></city>
</urban_area>
</district>
</geographical_area>
</municipality>
</location>
</administrative_district>
</region>
 mountain range of <region wordnetid="108630985" confidence="0.8">
<administrative_district wordnetid="108491826" confidence="0.8">
<location wordnetid="100027167" confidence="0.8">
<municipality wordnetid="108626283" confidence="0.8">
<land wordnetid="109334396" confidence="0.8">
<geographical_area wordnetid="108574314" confidence="0.8">
