<athlete wordnetid="109820263" confidence="0.8">
<link xlink:type="simple" xlink:href="../568/1266568.xml">
James Farrior</link></athlete>
</causal_agent>
</linebacker>
</contestant>
</player>
</football_player>
</person>
</physical_entity>
 and <physical_entity wordnetid="100001930" confidence="0.8">
