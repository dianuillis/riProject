<radical wordnetid="110503452" confidence="0.8">
<entertainer wordnetid="109616922" confidence="0.8">
<bad_person wordnetid="109831962" confidence="0.8">
<link xlink:type="simple" xlink:href="../301/98301.xml">
Joker</link></bad_person>
</entertainer>
</radical>
</killer>
</murderer>
