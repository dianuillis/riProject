<hamlet wordnetid="108226978" confidence="0.8">
<group wordnetid="100031264" confidence="0.8">
<link xlink:type="simple" xlink:href="../086/15688086.xml">
Dunvegan</link></group>
</hamlet>
</geographical_area>
</community>
</gathering>
</tract>
</location>
</social_group>
</region>
</site>
, Kimiwan Birdwalk and Interpretive Centre, <lake wordnetid="109328904" confidence="0.9508927676800064">
<link xlink:type="simple" xlink:href="../500/939500.xml">
Lesser Slave Lake</link></lake>
 Bird Observatory in the <area wordnetid="108497294" confidence="0.9508927676800064">
<link xlink:type="simple" xlink:href="../181/2652181.xml">
Lesser Slave Lake Provincial Park</link></area>
, Muskoseepi Park, <region wordnetid="108630985" confidence="0.8">
<field wordnetid="108569998" confidence="0.8">
<substance wordnetid="100019613" confidence="0.8">
<sand wordnetid="115019030" confidence="0.8">
<location wordnetid="100027167" confidence="0.8">
<tract wordnetid="108673395" confidence="0.8">
<geographical_area wordnetid="108574314" confidence="0.8">
<earth wordnetid="114842992" confidence="0.8">
